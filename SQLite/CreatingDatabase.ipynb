    "import sqlite3\n",
    "\n",
    "connection = sqlite3.connect('movies.db')\n",
    "cursor = connection.cursor()\n",
    "cursor.execute('''CREATE TABLE IF NOT EXISTS Movies\n",
    "            (Title TEXT, Director TEXT, Year INT)''')\n",
    "\n",
    "connection.commit()\n",
    "connection.close()"
